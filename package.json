{
  "name": "ephemera",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "vitest": "vitest",
    "predev": "npm run -w @app/sheaf-acl build",
    "db:push": "prisma db push",
    "post:db:push": "npm run gen:db:types",
    "migrate": "prisma migrate dev",
    "post:migrate": "npm run gen:db:types",
    "gen:db:types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public,graphql_public > supabase/database-supabase.ts",
    "prepare:db:types": "bash -lc '[ -n \"$SUPABASE_ACCESS_TOKEN\" ] && npm run gen:db:types || echo \"skip: no SUPABASE_ACCESS_TOKEN\"'",
    "prebuild": "npm run -w @app/sheaf-acl build",
    "postinstall": "prisma generate",
    "seed": "tsx --env-file=.env scripts/seed.ts",
    "gmail-token": "tsx --env-file=.env scripts/gmail-token.ts",
    "sheets-token": "tsx --env-file=.env scripts/google-sheets-token.ts",
    "watch:worker": "nodemon",
    "size": "size-limit",
    "backfill:block-thumbs": "tsx -r tsconfig-paths/register -r dotenv/config scripts/backfill-block-thumbnails.ts",
    "size:why": "size-limit --why",
    "worker": "tsx -r dotenv/config workers/index.ts",
    "acl:build": "tsc -p packages/sheaf-acl/tsconfig.json",
    "seed:discussion": "tsx scripts/seed-discussion-demo.ts",
    "seed:discussion:advanced": "tsx scripts/seed-discussion-advanced.ts",
    "seed:propositions": "tsx scripts/seed-propositions-demo.ts",
    "seed:propositions:quick": "tsx scripts/seed-propositions-quick.ts",
    "seed:propositions:clear": "tsx scripts/seed-propositions-demo.ts -- --clear",
    "seed:propositions:id": "tsx scripts/seed-propositions-demo.ts",
    "db:studio": "npx prisma studio",
    "acl:watch": "tsc -w -p packages/sheaf-acl/tsconfig.json",
    "acl:test": "vitest run packages/sheaf-acl",
    "sheaf:seed": "tsx --env-file=.env scripts/sheaf-seed.ts",
    "briefseed": "tsx --env-file=.env prisma/brief-seed.ts",
    "cq:backfill": "tsx --env-file=.env scripts/backfill-cq-responses.ts"
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/app/(root)/(standard)/page.js",
      "limit": "200000 kB",
      "webpack": true
    }
  ],
